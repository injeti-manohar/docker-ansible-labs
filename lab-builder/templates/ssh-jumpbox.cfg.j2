{% for item in lab_machines %}
Host {{ item.name }}
  HostName {{ item.ip }}

{%   endfor %}

Host 172.3.26.* 
  ProxyJump bastion

Host *
  User vagrant
  IdentityFile  ~/.vagrant.d/insecure_private_key
  ControlMaster auto
  ControlPath /tmp/%h-%r
  ControlPersist 5m
  UserKnownHostsFile /dev/null
  StrictHostKeyChecking no
